# Rename this file to .env

# Discord Configuration
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_CLIENT_ID=your_discord_application_id_here

# Channel IDs (right-click channel and "Copy ID" with Developer Mode enabled)
UPLOAD_CHANNELS=123456789012345678,987654321098765432
APPROVAL_CHANNEL_ID=555666777888999000

# Officer Role IDs (users with these roles can trigger uploads on any message)
# If not set, only original message authors can trigger uploads
OFFICER_ROLE_IDS=111222333444555666,777888999000111222

# Upload emoji (can be Unicode or custom emoji ID)
UPLOAD_EMOJI=⬆️

# Google Drive Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=urn:ietf:wg:oauth:2.0:oob
GOOGLE_REFRESH_TOKEN=your_google_refresh_token_here

# Default upload folder ID in Google Drive
DEFAULT_DRIVE_FOLDER_ID=your_default_folder_id_here 
